    "node-loader": "^0.6.0",
    "serverless": "^1.27.3",
    "serverless-plugin-write-env-vars": "^1.0.2",
    "serverless-webpack": "^5.1.5",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4"
  }
}
